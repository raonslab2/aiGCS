	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/> 
	<typeAlias  alias="gA00MAINVO" type="egovframework.or.kr.GA00MAIN.service.GA00MAINVO"/> 
 
 	<resultMap id="GmainAirList" class="egovframework.or.kr.GA00MAIN.service.GA00MAINVO">
