<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="EMA00H01">

	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/> 
	<typeAlias  alias="eMA00H01VO" type="egovframework.or.kr.EMA00H01.service.EMA00H01VO"/> 
 
	<resultMap id="selectEMA00H01List" class="egovframework.or.kr.EMA00H01.service.EMA00H01VO">
		<result property="h01Id" column="H01_ID" columnIndex="1"/>
		<result property="h01NameCode" column="H01_NAME_CODE" columnIndex="2"/>
		<result property="h01Name" column="H01_NAME" columnIndex="3"/> 
	</resultMap> 
	<select id="EMA00H01.selectEMA00H01" parameterClass="EMA00H01VO" resultMap="selectEMA00H01List" >
			<![CDATA[
				 SELECT 
				      a.H01_ID
				     ,a.H01_NAME_CODE
				     ,a.H01_NAME
				 FROM EMA00H01 a
					]]>
				<![CDATA[
			 		ORDER BY a.H01_ID desc 
			 		LIMIT #recordCountPerPage# OFFSET #firstIndex#
				]]>	
	</select>
	 
	<select id="EMA00H01.selectEMA00H01Cnt" parameterClass="EMA00H01VO" resultClass="int" >
		<![CDATA[      
			SELECT     
	        	COUNT(*)
		    FROM
				EMA00H01 a
		]]>
 	
	</select>
 
</sqlMap>
